<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="8dip" />
            <stroke android:width="2dip" android:color="#ffffffff" />
            <gradient  android:angle="-90"  android:startColor="#ffffffff" android:endColor="#ffffffff"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="8dip" />
            <stroke android:width="2dip" android:color="#ffffffff" />
            <solid  android:color="#ffffffff"/>
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle"  >
            <corners android:radius="8dip" />
            <stroke android:width="2dip" android:color="#ffffffff" />
            <solid  android:color="#ff767472"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="8dip" />
            <stroke android:width="4dip" android:color="#ffffffff" />
            <gradient  android:angle="-45"  android:startColor="#ff005596" android:endColor="#ff0080e3" />
        </shape>
    </item>
</selector>